def main():
    arr = [1, 3, 8, 2, 24, 9, 11]
    print(bubblesort(arr))




def bubblesort(array):
    for i in range(len(array)):
        for j in range(len(array)-1):
            if array[j] > array[j+1]:
                temp = array[j+1]
                array[j+1] = array[j]
                array[j] = temp
    return array

if __name__ == "__main__":
    main()
